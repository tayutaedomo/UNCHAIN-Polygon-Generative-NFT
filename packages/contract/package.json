{
  "name": "contract",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/contracts": "^4.9.3",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "chai": "^4.3.7",
    "ethers": "5.7.0",
    "hardhat": "^2.13.0",
    "hardhat-gas-reporter": "^1.0.9",
    "solidity-coverage": "^0.8.2",
    "typechain": "^8.1.1"
  },
  "scripts": {
    "run:script": "npx hardhat run scripts/run.js",
    "test": "npx hardhat test",
    "compile": "npx hardhat compile",
    "deploy:sepolia": "npx hardhat run scripts/deploy.js --network sepolia",
    "deploy:mumbai": "npx hardhat run scripts/deploy.js --network mumbai",
    "verify:sepolia": "npx hardhat verify --network sepolia $DEPLOYED_CONTRACT_ADDRESS $BASE_TOKEN_URI",
    "verify:mumbai": "npx hardhat verify --network mumbai $POLYGON_DEPLOYED_CONTRACT_ADDRESS $BASE_TOKEN_URI",
    "start": "npx hardhat node"
  }
}
